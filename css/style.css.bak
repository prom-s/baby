/*全局样式*/
p,body,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd{
	margin: 0px;
	padding: 0px;
}
body{
	font-size: 12px;
	font-family: "微软雅黑";
}
img{
	vertical-align: bottom;
}
ol,ul{
	list-style: none;
}
a{
	color: #666;
	text-decoration: none;
}
a:hover{
	color: #FF5C00;
}
.lf{
	float: left;
}
.rt{
	float: right;
}
.clear{
	clear: both;
}
/**********头部*************/
#headerAll{
	height: 40px;
	border-bottom: 1px solid #d7d6d7;
	background-image: linear-gradient(180deg,#fcfafc 0%, #f1f0f2 100%);
}
#header{
	width: 1050px;
	margin: 0 auto;
}
#header .headerLeft{
	float: left;
	margin-top: 5px;
}
#header .headerLeft li{
	width: 96px;
	height: 36px;
	line-height: 36px;
	text-align: center;
	font-weight: bolder;
	float: left;
	border-right: 1px solid #e7e7e7;
}
.phone{
	width: 79px;
	height: 40px;
	padding-left: 8px;
	padding-right: 8px;
	position: relative;
}
.phone a{
	border: 2px solid transparent;
	display: block;
	border-bottom: none;
	height: 36px;
	position: relative;
	z-index: 10;
}
.phonem{
	width: 278px;
	height: 105px;
	border: 2px solid #ff8a00;
	position: absolute;
	top: 36px;
	left: 8px;
	background: #FFFFFF;
	margin-top: -1px;
	display: none;
}
#header .headerLeft li a:hover{
	color: #ff8a00;
	text-decoration: none;
}
.phone a:hover{
	background: #FFFFFF;
	border: 2px solid #FF8A00;
	border-bottom: none;
}
#header .headerLeft u{
	width: 14px;
	height: 22px;
	display: inline-block;
	float: left;
	background: url(../img/img3/sprite27new.png)no-repeat -320px -240px;
	position: relative;
	top: 4px;
	left: 20px;
	margin-right: 11px;
}

#header .headerLeft1{
	border: 1px solid #d7d6d7;
	border-bottom: none;
	background: #fff;
}
#header .headerLeft1 a{
	color: #ff8a00;
}
.headerRight{
	float: right;
	height: 36px;
	line-height: 36px;
	margin-top: 5px;
}
.headerRight li{
	float: left;
	margin-left: 10px;
}
.headerRight s{
	width: 1px;
	height: 14px;
	background: #cccccc;
	float: left;
	display: inline-block;
	margin-top: 12px;
	margin-left: 8px;
}
.headerRight i{
	width: 0px;
	height: 0px;
	border: 4px solid transparent;
	border-top: 4px solid #666666;
	display: inline-block;
	margin-left: 4px;
	position: relative;
	top: 3px;
}
.mydd {
	width: 60px;
	height: 36px;
	/*padding-left: 4px;*/
	padding-right: 4px;
	position: relative;
	margin-left: -1px;
}
.mydd a{
	width: 70px;
	height: 36px;
	border: 2px solid transparent;
	border-bottom: none;
	display:inline-block;
	position: relative;
	z-index: 30;
	color: #666666;
	
}
.mydd a:hover{
	border: 2px solid #FF8A00;
	background: #FFFFFF;
	border-bottom: none;
}
.mydd:hover .myddm{
	display: block;
}
.mydd:hover i{
	width: 0px;
	height: 0px;
	border: 4px solid transparent;
	border-bottom: 4px solid #FF8A00;
	display: inline-block;
	margin-left: 4px;
	position: relative;
	top: 3px;
}
.mydd .myddm{
	width:70px;
	height: 107px;
	border: 2px solid #FF8A00;
	position: absolute;
	top: 31px;
	left: 0px;
	z-index: 20;
	display: none;
	background: #FFFFFF;
}
.mydd .myddm ul li{
	line-height: 22px;
	margin-left: 0px;
}
.khfw {
	width: 60px;
	height: 36px;
	/*padding-left: 4px;*/
	padding-right: 4px;
	position: relative;
	margin-left: -1px;
}
.khfw a{
	width: 70px;
	height: 36px;
	border: 2px solid transparent;
	border-bottom: none;
	display:inline-block;
	position: relative;
	z-index: 20;
	color: #666666;
	
}
.khfw a:hover{
	border: 2px solid #FF8A00;
