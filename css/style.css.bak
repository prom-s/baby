	border-bottom: none;
}
.khfw:hover .khfwm{
	display: block;
}
.khfw:hover i{
	width: 0px;
	height: 0px;
	border: 4px solid transparent;
	border-bottom: 4px solid #FF8A00;
	display: inline-block;
	margin-left: 4px;
	position: relative;
	top: 3px;
}
.khfw .khfwm{
	width:88px;
	height: 49px;
	border: 2px solid #FF8A00;
	position: absolute;
	top: 36px;
